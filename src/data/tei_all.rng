      <sch:pattern id="tei_all-moduleRef-modref-constraint-rule-17">
            <sch:rule context="tei:moduleRef">
               <sch:report test="* and @key">
Child elements of <sch:name/>
 are only allowed when an external module is being loaded
        </sch:report>

            </sch:rule>

         </sch:pattern>
      <ref name="att.global.attributes"/>
      <optional>
        <attribute name="prefix">
