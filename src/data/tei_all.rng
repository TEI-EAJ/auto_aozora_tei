      <ref name="att.global.attributes"/>
      <optional>
        <attribute name="type">
          <a:documentation>即興度の種類を示すキーワード．
例としての値は以下の通り: 1] none; 2] scripted; 3] formulaic; 4] revised</a:documentation>
          <data type="token">
            <param name="pattern">(\p{L}|\p{N}|\p{P}|\p{S})+</param>
          </data>
        </attribute>
      </optional>
      <empty/>
