          <choice>
            <ref name="model.pLike"/>
            <ref name="decoNote"/>
            <ref name="condition"/>
          </choice>
        </oneOrMore>
        <oneOrMore>
          <ref name="binding"/>
        </oneOrMore>
      </choice>
      <ref name="att.global.attributes"/>
      <empty/>
