          <ref name="model.graphicLike"/>
          <ref name="model.hiLike"/>
        </choice>
      </zeroOrMore>
      <ref name="att.global.attributes"/>
      <ref name="att.notated.attributes"/>
      <empty/>
    </element>
  </define>
  <define name="notatedMusic">
