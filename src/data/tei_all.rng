            <data type="boolean"/>
            <choice>
              <value>unknown</value>
              <a:documentation/>
              <value>inapplicable</value>
              <a:documentation/>
            </choice>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="follow">
          <a:documentation>当該ノードの姉要素の識別子を示す．</a:documentation>
