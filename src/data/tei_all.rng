      <ref name="att.global.attributes"/>
      <ref name="att.lexicographic.attributes"/>
      <optional>
        <attribute name="type">
          <a:documentation>当該ラベルの分類を示す．</a:documentation>
          <data type="token">
            <param name="pattern">(\p{L}|\p{N}|\p{P}|\p{S})+</param>
          </data>
