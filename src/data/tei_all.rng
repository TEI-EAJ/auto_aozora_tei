        <oneOrMore>
          <ref name="model.pLike"/>
        </oneOrMore>
        <group>
          <optional>
            <ref name="summary"/>
          </optional>
          <oneOrMore>
            <ref name="handNote"/>
          </oneOrMore>
        </group>
      </choice>
      <ref name="att.global.attributes"/>
      <optional>
        <attribute name="hands">
          <a:documentation>当該手書き資料中で特定可能な筆致の数を示す．</a:documentation>
          <data type="nonNegativeInteger"/>
        </attribute>
      </optional>
      <empty/>
    </element>
  </define>
  <define name="typeDesc">
    <element name="typeDesc">
      <a:documentation>contains a description of the typefaces or other aspects of the printing of an incunable or other printed source. [10.7.2.1. Writing]</a:documentation>
      <choice>
        <oneOrMore>
          <ref name="model.pLike"/>
        </oneOrMore>
        <group>
          <optional>
            <ref name="summary"/>
          </optional>
          <oneOrMore>
            <ref name="typeNote"/>
          </oneOrMore>
        </group>
      </choice>
      <ref name="att.global.attributes"/>
      <empty/>
