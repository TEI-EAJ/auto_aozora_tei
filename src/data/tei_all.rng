            <choice>
              <ref name="leaf"/>
              <ref name="iNode"/>
            </choice>
          </zeroOrMore>
          <ref name="root"/>
          <zeroOrMore>
            <choice>
              <ref name="leaf"/>
              <ref name="iNode"/>
            </choice>
          </zeroOrMore>
        </group>
      </group>
      <ref name="att.global.attributes"/>
