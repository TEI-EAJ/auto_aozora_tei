          <choice>
            <value>high</value>
            <a:documentation/>
            <value>medium</value>
            <a:documentation/>
            <value>low</value>
            <a:documentation/>
            <value>unknown</value>
            <a:documentation/>
          </choice>
        </attribute>
      </optional>
      <empty/>
