        <choice>
          <ref name="model.descLike"/>
          <ref name="model.certLike"/>
        </choice>
      </zeroOrMore>
      <ref name="att.global.attributes"/>
      <ref name="att.scoping.attributes"/>
      <ref name="att.ranging.attributes"/>
      <ref name="att.datable.attributes"/>
      <optional>
        <attribute name="precision">
          <a:documentation>characterizes the precision of the element or attribute pointed to by the precision element.</a:documentation>
          <choice>
            <value>high</value>
            <a:documentation/>
            <value>medium</value>
            <a:documentation/>
            <value>low</value>
            <a:documentation/>
            <value>unknown</value>
            <a:documentation/>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="stdDeviation">
          <a:documentation>supplies a standard deviation associated with the value in question</a:documentation>
          <choice>
            <data type="double"/>
            <data type="token">
              <param name="pattern">(\-?[\d]+/\-?[\d]+)</param>
            </data>
            <data type="decimal"/>
