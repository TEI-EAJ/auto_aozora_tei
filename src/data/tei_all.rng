      <sch:pattern id="tei_all-span-targetfrom-constraint-report-13">
            <sch:rule context="tei:span">
               <sch:report test="@from and @target">
Only one of the attributes @target and @from may be supplied on <sch:name/>

               </sch:report>

            </sch:rule>

         </sch:pattern>
      <sch:pattern id="tei_all-span-targetto-constraint-report-14">
            <sch:rule context="tei:span">
               <sch:report test="@to and @target">
Only one of the attributes @target and @to may be supplied on <sch:name/>

               </sch:report>

            </sch:rule>

         </sch:pattern>
      <sch:pattern id="tei_all-span-tonotfrom-constraint-report-15">
            <sch:rule context="tei:span">
               <sch:report test="@to and not(@from)">
If @to is supplied on <sch:name/>
, @from must be supplied as well</sch:report>

            </sch:rule>

         </sch:pattern>
      <sch:pattern id="tei_all-span-tofrom-constraint-report-16">
            <sch:rule context="tei:span">
               <sch:report test="contains(normalize-space(@to),' ') or contains(normalize-space(@from),' ')">
The attributes @to and @from on <sch:name/>
 may each contain only a single value</sch:report>

            </sch:rule>

         </sch:pattern>
      <ref name="att.global.attributes"/>
      <ref name="att.interpLike.attributes"/>
      <ref name="att.pointing.attributes"/>
      <optional>
        <attribute name="from">
          <a:documentation>当該注釈対象の始点を示す．属性toがない場合は，対象全 体を示す．</a:documentation>
          <data type="anyURI"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="to">
          <a:documentation>注釈対象の終点を示す．</a:documentation>
          <data type="anyURI"/>
        </attribute>
      </optional>
      <empty/>
    </element>
  </define>
  <define name="spanGrp">
