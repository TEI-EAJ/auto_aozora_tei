          <optional>
            <ref name="summary"/>
          </optional>
          <optional>
            <ref name="origin"/>
          </optional>
          <zeroOrMore>
            <ref name="provenance"/>
          </zeroOrMore>
          <optional>
            <ref name="acquisition"/>
          </optional>
