        <data type="anyURI"/>
      </attribute>
    </optional>
  </define>
  <define name="att.global.linking.attribute.copyOf">
    <optional>
      <attribute name="copyOf">
        <a:documentation>当該要素のコピー要素を示す．</a:documentation>
        <data type="anyURI"/>
      </attribute>
    </optional>
  </define>
  <define name="att.global.linking.attribute.next">
    <optional>
      <attribute name="next">
        <a:documentation>当該要素も所属する仮想集合における次の要素を示す．</a:documentation>
