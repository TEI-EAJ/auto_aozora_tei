            <choice>
              <ref name="decoNote"/>
              <ref name="seal"/>
              <ref name="condition"/>
            </choice>
          </oneOrMore>
        </group>
      </choice>
      <ref name="att.global.attributes"/>
      <empty/>
    </element>
